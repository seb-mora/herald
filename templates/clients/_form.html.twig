{{form_errors(form)}}

{{ form_start(form) }}

    <div class="p-2">
        {{ form_row(form.nom) }}
    </div>
    <div class="p-2">
        {{ form_row(form.adresse) }}
    </div>
    <div class="p-2">
        {{ form_row(form.telephone) }}
    </div>

    <button class="btn border-2 border-emerald-500 hover:bg-emerald-500 hover:font-bold hover:text-white duration-200">{{ button_label|default('Enregistrer') }}</button>

{{ form_end(form) }}
